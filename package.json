{
  "name": "blocks-util-js",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "style": "lib/index.css",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run style && babel ./src -d ./lib",
    "start": "rollup -c -w",
    "clean": "rimraf lib/*",
    "sass": "node-sass scss/index.scss lib/index.css",
    "style": "npm run sass && cssnano lib/index.css lib/index.min.css --no-zindex --no-reduceIdents",
    "storybook": "npm run sass && start-storybook -p 8008",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tengine/blocks-util-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tengine/blocks-util-js/issues"
  },
  "homepage": "https://github.com/tengine/blocks-util-js#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "react": "^16.3.2",
    "react-dom": "16.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "cssnano-cli": "^1.0.5",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0"
  }
}
